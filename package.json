{
  "name": "@ogauk/react-tiny-editor",
  "version": "0.1.12",
  "description": "A tiny HTML rich text editor for React",
  "type": "module",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "pretest": "eslint src",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "build": "rm -r lib && tsc && cp src/index.css lib",
    "start": "webpack-dev-server --open",
    "postversion": "git push && git push --tags && npm publish",
    "prepublish": "build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jcable/react-tiny-editor.git"
  },
  "keywords": [
    "tiny",
    "editor",
    "wysiwyg",
    "html",
    "react",
    "javascript"
  ],
  "author": "jcable",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcable/react-tiny-editor/issues"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.21",
    "@types/sanitize-html": "^2.9.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0"
  },
  "optionalDependencies": {
    "sanitize-html": "^2.11.0"
  }
}
